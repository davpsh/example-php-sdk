{
    "name": "example/example-php-sdk",
    "description": "Example PHP SDK.",
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^7",
        "apimatic/jsonmapper": "^2.0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "Example\\": "src/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "lint": "phpcs --standard=phpcs-ruleset.xml src/"
    }
}
